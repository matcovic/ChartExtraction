{
  "system": {
    "dataset": "LineCls",
    "batch_size": 10,
    "sampling_function": "kp_detection",

    "train_split": "trainchart",
    "val_split": "valchart",
    "test_split": "testchart",

    "learning_rate": 0.00025,
    "decay_rate": 10,

    "val_iter": 10,

    "opt_algo": "adam",
    "prefetch_size": 5,

    "max_iter": 1000,
    "stepsize": 400,
    "snapshot": 200,

    "chunk_sizes": [5, 7, 7, 7],

    "data_dir": "./data/linedata(1028)/",
    "cache_dir": "./data/linedata(1028)/cache/"
  },

  "db": {
    "rand_scale_min": 0.6,
    "rand_scale_max": 1.4,
    "rand_scale_step": 0.1,
    "rand_scales": null,

    "rand_crop": true,
    "rand_color": true,

    "border": 128,
    "gaussian_bump": true,
    "gaussian_iou": 0.3,

    "input_size": [511, 511],
    "output_sizes": [[128, 128]],

    "test_scales": [1],

    "top_k": 100,
    "categories": 6,
    "ae_threshold": 0.5,
    "nms_threshold": 0.5,

    "max_per_image": 100
  }
}
